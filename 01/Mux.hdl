/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=sel,b=sel,out=temp);
    Nand(a=a,b=temp,out=temp0);
    Nand(a=sel,b=b,out=temp1);
    Nand(a=temp0,b=temp1,out=out);
    
}